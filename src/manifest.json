{
  "manifest_version": 2,

  "name": "Alfresco Calendar Upgrade",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "content_scripts" : [{
      "matches" : [
        "http://www.htmlgoodies.com/*",
        "*://alfresco.luxfacta.com.br/share/page/site/*/calendar*"
      ],
      "js" : ["assets/jquery-1.12.3.js", "calendar.js"]
    }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "web_accessible_resources": [
    "assets/jquery-1.12.3.js",
    "calendar.js"
  ],
  "background": {
    "scripts" : ["background.js"]
  }
}